<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zking.mapper.fwgl.IServiceHandlingMapper">
    <select id="findAll" resultType="ServiceHandling" parameterType="ServiceHandling">
         select * from  ServiceHandling
         <where>
             <if test="Dispatch!=null">
                 and Dispatch = #{Dispatch}
             </if>
             <if test="StDescribe!=null">
                 and StDescribe =#{StDescribe}
             </if>
             <if test="CustId!=null">
                 and CustId =#{CustId}
             </if>
             <if test="CustAddress!=null">
                 and CustId =( select CustId from x_customermessage where CustAddress like '%${Other%}%')
             </if>
             <if test="startTime!=null">
                 and AcceptDate > #{startTime}
             </if>
             <if test="endTime!=null">
                 and AcceptDate > #{endTime}
             </if>
         </where>
        limit #{page},#{limit}
    </select>
    <select id="getMax" resultType="ServiceHandling" parameterType="java.lang.Integer">
        select count(ShId) FROM  ServiceHandling
        <where>
            <if test="Dispatch!=null">
                and Dispatch = #{Dispatch}
            </if>
            <if test="StDescribe!=null">
                and StDescribe =#{StDescribe}
            </if>
            <if test="CustId!=null">
                and CustId =#{CustId}
            </if>
            <if test="CustAddress!=null">
                and CustId =( select CustId from x_customermessage where CustAddress like '%${Other}%')
            </if>
            <if test="startTime!=null">
                and AcceptDate > #{startTime}
            </if>
            <if test="endTime!=null">
                and AcceptDate > #{endTime}
            </if>
        </where>
    </select>
    <select id="findonly" resultType="ServiceHandling" parameterType="java.lang.String">
        select * from servicehandling where ShId=#{ShId}
    </select>
    <insert id="add" parameterType="ServiceHandling">

    </insert>
    <update id="edit" parameterType="ServiceHandling">

    </update>
    <delete id="remove" parameterType="ServiceHandling">

    </delete>
    <select id="getEnd" >
        SELECT MAX(SUBSTR(ShId, -4))+1 FROM ServiceHandling
    </select>
    <select id="findByListId" parameterType="ServiceHandling" resultType="java.lang.String">
        select * from ServiceHandling where ShId in (#{ListId})
    </select>
</mapper>
